/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.clearfix:after { content: "\0020";	display: block;	height: 0;	clear: both; visibility: hidden}
.clearfix {clear: both; zoom: 1}
.navigator-hover{background: none;}


/* 尾部 */
.public_footer{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 50px;
  border-top: 1px solid #f5f5f5;
  background: #fff;
  z-index: 100;
}
.public_footer .f_nav{
  width: 20%;
  float: left;
  text-align: center;
  font-size: 0.6rem;
  color: #b6b6b6;
  margin-top: 8px;
}
.public_footer .f_nav icon{
  width: 20px;
  height: 20px;
}
.public_footer .f_nav text{
  display: block;
}
.public_footer .f_nav.active text{
  color: #e30c26;
}