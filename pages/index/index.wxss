.index_header .index_logo{
  width: 22%;
  text-align: center;
  float: left;
}
.index_header .index_logo image{
  width: 82px;
  height: 25px;
  line-height: 50px;
  margin: 12px 5px;
}
.index_header .index_search{
  width: 78%;
  float: left;
  padding: 10px;
  box-sizing: border-box;
}
.index_header .index_search input{
  display: inline-block;
  width: 100%;
  border: 1px solid #f5f5f5;
  padding: 3px 8px;
  box-sizing: border-box;
  font-size: 0.75rem;
  /* background: rgba(255, 255, 255, 0.8); */
  background: #fff;
  height: 30px;
}
